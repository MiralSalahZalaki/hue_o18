<odoo>
    <data>
        <record id="mc_res_company_form_inherit" model="ir.ui.view">
            <field name="name">mc.res.company.form.inherit</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='phone']" position="after">
                    <group>
                        <field name="school_identifier" />
                    </group>

                </xpath>
            </field>
        </record>

    </data>
</odoo>




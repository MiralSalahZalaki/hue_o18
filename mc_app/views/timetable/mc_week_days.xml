<odoo>
    <record id="mc_week_days_list_view" model="ir.ui.view">
        <field name="name">mc_week_days_list</field>
        <field name="model">mc.week.days</field>
        <field name="arch" type="xml">
            <list string="Week days" editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>
            
    <record id="action_mc_week_days" model="ir.actions.act_window">
        <field name="name">Week Days</field>
        <field name="res_model">mc.week.days</field>
        <field name="view_mode">list</field>
    </record>



     <menuitem
            id="week_days_sub_menu"
            name="Week Days"
            parent="education_time_table.education_timetable_menu_configuration"
            action="action_mc_week_days"/>


</odoo>

<odoo>
    <data>
        <record id="mc_prev_school_type_tree_view" model="ir.ui.view">
            <field name="name">mc_prev_school_type_tree_view</field>
            <field name="model">mc.prev.school.type</field>
             <field name="arch" type="xml">
                <list>
                    <field name="name"/>
                </list>
            </field>
        </record>


        <record id="mc_prev_school_type_form_view" model="ir.ui.view">
            <field name="name">mc_prev_school_type_form_view</field>
            <field name="model">mc.prev.school.type</field>
             <field name="arch" type="xml">
                <form>
                    <field name="name"/>
                </form>
            </field>
        </record>

        
        <record id="action_mc_prev_school_type" model="ir.actions.act_window">
            <field name="name">Prev Schools Types</field>
            <field name="res_model">mc.prev.school.type</field>
            <field name="view_mode">list,form</field>
        </record>



        <menuitem id="menu_mc_prev_school_type"
                name="Prev Schools Types" 
                parent="menu_application_class_setup"
                action="action_mc_prev_school_type"/>

    </data>
</odoo>
